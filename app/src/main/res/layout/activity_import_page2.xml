<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".import_page2">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.5"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/bg_image" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginVertical="10dp"
        android:layout_marginHorizontal="10dp"
        app:srcCompat="@drawable/rectangle_12"/>

    <TextView
        android:id="@+id/textView30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/inter_extrabold"
        android:text="@string/designnew_text"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        app:layout_constraintTop_toTopOf="@id/imageView7"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        app:layout_constraintEnd_toEndOf="@id/imageView7"
        tools:layout_editor_absoluteX="40dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="405dp"
        android:layout_marginHorizontal="95dp"
        android:background="@drawable/edit_text_bg"
        android:textColor="@color/white"
        android:text="View in AR"
        android:textColorLink="#303C53"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        app:layout_constraintTop_toTopOf="@id/imageView7" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:scrollbarSize="40dp"
        android:layout_marginTop="15dp"
        android:text="Feedbacks"
        android:textSize="20dp"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        app:layout_constraintEnd_toEndOf="@id/imageView7"
        app:layout_constraintTop_toBottomOf="@id/button7" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:textSize="20dp"
        android:scrollbarSize="40dp"
        android:text="On this Design!"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        app:layout_constraintEnd_toEndOf="@id/imageView7"
        app:layout_constraintTop_toBottomOf="@id/textView33" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="235dp"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="10dp"
        android:padding="12dp"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        app:layout_constraintTop_toBottomOf="@id/textView30" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="303dp"
        android:layout_height="226dp"
        android:background="@drawable/rectangle_27"
        app:layout_constraintTop_toBottomOf="@id/textView34"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="@id/imageView7"
        android:layout_marginHorizontal="30dp"
        app:layout_constraintEnd_toEndOf="@id/imageView7"
        />

</androidx.constraintlayout.widget.ConstraintLayout>

