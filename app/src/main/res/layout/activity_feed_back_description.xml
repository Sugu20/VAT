<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedBackDescription">

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:alpha="0.8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bggg" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle_12" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/inter_extrabold"
        android:text="FeedBack Descrption"
        android:textColor="#FEFACA"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="@id/imageView25"
        app:layout_constraintTop_toTopOf="@id/imageView26" />

    <ImageView
        android:id="@+id/ImageviewFeedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="25dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@id/textView39"/>

    <TextView
        android:id="@+id/GivenBy"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/ImageviewFeedback"
        android:layout_marginTop="25dp"
        android:layout_marginHorizontal="30dp"
        app:layout_constraintStart_toStartOf="@id/imageView25"
        app:layout_constraintEnd_toEndOf="@id/imageView25"
        android:textColor="#FEFACA"
        android:textSize="20dp"
        android:text="FeedBack Given By : "/>

    <androidx.appcompat.widget.AppCompatMultiAutoCompleteTextView
        android:id="@+id/Feedbackshown"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        app:layout_constraintTop_toBottomOf="@id/GivenBy"
        android:background="@drawable/rectangle_84"
        android:layout_marginHorizontal="30dp"
        app:layout_constraintStart_toStartOf="@id/imageView25"
        app:layout_constraintEnd_toEndOf="@id/imageView25"
        android:layout_marginTop="30dp"
        android:gravity="top|start"
        android:textSize="15dp"
        android:textColor="@color/black"/>

    <FrameLayout
        android:id="@+id/Homebtn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:visibility="invisible"
        app:layout_constraintTop_toBottomOf="@id/Feedbackshown"
        app:layout_constraintStart_toStartOf="@id/imageView25"
        app:layout_constraintEnd_toEndOf="@id/imageView25"
        >

        <ImageButton
            android:id="@+id/imageView22"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            app:srcCompat="@drawable/baseline_home_24"
            android:scaleType="fitCenter"
            android:contentDescription="Home button"
            />
    </FrameLayout>

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:background="@null"
        android:contentDescription="Back"
        app:layout_constraintEnd_toStartOf="@id/Homebtn"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Feedbackshown"
        app:srcCompat="@drawable/back" />

</androidx.constraintlayout.widget.ConstraintLayout>