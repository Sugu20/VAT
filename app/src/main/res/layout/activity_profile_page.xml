<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile_page">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.5"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/bg_image" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rectangle_12"
        android:scaleType="fitXY"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        />


    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_80"
        app:layout_constraintStart_toStartOf="@id/imageView13"
        app:layout_constraintTop_toBottomOf="@id/textView35"
        app:layout_constraintEnd_toEndOf="@id/imageView13"/>

    <TextView
        android:id="@+id/textView35"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"

        android:fontFamily="@font/inter_bold"
        android:text="Profile."
        android:textColor="#FFFFFF"
        android:textSize="40dp"
       android:layout_marginHorizontal="40dp"
        app:layout_constraintTop_toTopOf="@id/imageView13" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="263dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextText8" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="263dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextTextEmailAddress" />

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="257dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toTopOf="@id/imageView16" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="263dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextTextEmailAddress" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="263dp"
        android:layout_height="52dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextDate" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="804dp"
        android:text="LOGOUT"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginHorizontal="90dp"
        android:background="@drawable/edit_text_bg"
        android:textColor="@color/white"
        android:text="Uploaded Designs."
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextTextMultiLine" />


</androidx.constraintlayout.widget.ConstraintLayout>