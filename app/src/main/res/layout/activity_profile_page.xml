<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile_page">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0.8"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/bggg" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/rectangle_12"
        android:scaleType="fitXY"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        />


    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle_80"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="20dp"
        app:layout_constraintStart_toStartOf="@id/imageView13"
        app:layout_constraintTop_toBottomOf="@id/textView35"
        app:layout_constraintEnd_toEndOf="@id/imageView13"/>

    <TextView
        android:id="@+id/textView35"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_bold"
        android:text="Profile."
        android:textColor="#FEFACA"
        android:textSize="40dp"
        android:gravity="center"
       android:layout_marginHorizontal="40dp"
        app:layout_constraintTop_toTopOf="@id/imageView13" />

    <TextView
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="25dp"
        android:ems="10"
        android:textSize="19dp"
        android:text="Email :"
        android:layout_marginHorizontal="50dp"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextText8" />

    <TextView
        android:id="@+id/editTextText8"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="25dp"
        android:ems="10"
        android:text="Name :"
        android:textSize="19dp"
        android:layout_marginHorizontal="50dp"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toTopOf="@id/imageView16" />

    <TextView
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginHorizontal="50dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="19dp"
        android:text="Phone number :"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:text="LOGOUT"
        android:textColor="@color/black"
        android:layout_marginHorizontal="100dp"
        android:background="@drawable/edit_text_bg"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/button10" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="90dp"
        android:layout_marginTop="130dp"
        android:background="@drawable/edit_text_bg"
        android:text="Uploaded Designs."
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView16"
        app:layout_constraintTop_toBottomOf="@id/editTextTextMultiLine" />

    <FrameLayout
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/imageView16"
        app:layout_constraintStart_toStartOf="@id/imageView13"
        app:layout_constraintEnd_toEndOf="@id/imageView13"
        >

        <ImageButton
            android:id="@+id/imageView22"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@null"
            android:contentDescription="Home button"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/baseline_home_24" />
    </FrameLayout>


</androidx.constraintlayout.widget.ConstraintLayout>